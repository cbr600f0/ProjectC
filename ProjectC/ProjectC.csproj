      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>RegisterPage.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\SecurityQuestionEnum.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>SecurityQuestionEnum.Designer.cs</LastGenOutput>
    </EmbeddedResource>
