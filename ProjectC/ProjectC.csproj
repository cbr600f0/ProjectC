    </Compile>
    <Compile Update="Resources\LoginPage.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>LoginPage.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\RegisterPage.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>RegisterPage.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\SecurityQuestionEnum.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
