    </Compile>
    <Compile Update="Pages\HogeScoresPage.xaml.cs">
      <DependentUpon>HogeScoresPage.xaml</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Pages\ChangePasswordPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
